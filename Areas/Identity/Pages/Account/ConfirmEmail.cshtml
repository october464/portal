@page
@model ConfirmEmailModel
@{
    ViewData["Title"] = "Confirm email";
}

<div class="container">

    <div class="card">
        <div class="card-header">
        
            <h1>@ViewData["Title"]</h1>
        </div>
        <div class="card-body">
            <h3>Thanks for confirming your email. To continue login</h3>
            <a class="btn btn-primary" asp-controller="Home" asp-action="Index">Back To Login</a>
        </div>
    </div>
</div>